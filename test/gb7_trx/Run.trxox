#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

if( ! $?TESTsander ) set TESTsander = "../../bin/sander"

if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
endif

set prmtop = "prmtop"
if( $#argv > 0 ) then
   set prmtop = "$1"
endif

cat > gbin <<EOF
 test of gen. Born, JM&AO parms
 &cntrl                                                                        
   imin=1, maxcyc=5,
   cut=99.0, igb=7, saltcon=0.2, gbsa=1,
   ntpr=1, ntx=1, ntb=0, ig=71277,
 /
EOF

set output = mdout.trxox

$DO_PARALLEL $TESTsander -O -p $prmtop -i gbin -c trxox.2.4ns.x  -o $output || goto error

../dacdif $output.save $output
/bin/rm -f gbin restrt mdinfo
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)
